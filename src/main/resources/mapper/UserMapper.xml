<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.example.lsmock.mapper.UserMapper">

    <select id="findAll" resultType="com.example.lsmock.dao.User">
        select * from user
    </select>

    <select id="findByName" parameterType="com.example.lsmock.dao.User" resultType="java.lang.String">
        select password from user WHERE name=#{name}
    </select>

</mapper>